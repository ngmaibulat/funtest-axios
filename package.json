        "url": "https://github.com/ngmaibulat/funtest-axios/issues"
    },
    "homepage": "https://github.com/ngmaibulat/funtest-axios#readme",
    "dependencies": {
        "@aibulat/isfile": "^0.0.3",
        "@aibulat/json": "^0.0.8",
        "@aibulat/run": "^0.0.6",
        "@types/axios": "^0.14.0",
        "axios": "^1.1.3"
    },
    "devDependencies": {
        "@types/jest": "^29.2.3",
        "jest": "^29.3.1"
